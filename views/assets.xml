<openerp>
  <data>

    <template id="assets_frontend_css" name="My Snippets Assets" inherit_id="website.assets_frontend">
      <xpath expr="." position="inside">
        <link href="/my_snippet/static/src/css/style.css" rel="stylesheet" type="text/css"/>
      </xpath>
    </template>

    <template id="assets_editor" inherit_id="website.assets_editor">
        <xpath expr="." position="inside">
            <script src="/my_snippet/static/src/js/script.js"></script>
        </xpath>
    </template>

  </data>
</openerp>