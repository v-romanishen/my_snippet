<odoo>
<template id="my_snippet_bar" inherit_id="website.snippets" name="My Snippet Bar">
	<xpath expr="//div[@id='snippet_structure']" position="after">
		<div id="my_snippet" class="o_panel">
			<div class="o_panel_header">
				<i class="fa fa-th"></i>
				My Snippet
			</div>
			<div class="o_panel_body">
	
			</div>
		</div>
	</xpath>
</template>

<template id="my_snippet_icon" inherit_id="my_snippet_bar" name="My Snippet Icon">
  <xpath expr="//div[@id='my_snippet']/div[@class='o_panel_body']" position="inside">
    <t t-snippet="my_snippet.snippet_my_snippet"
       t-thumbnail="/my_snippet/static/src/img/thumbnail.png"/>
  </xpath>
</template>

<template id="four_blocks_icon" inherit_id="my_snippet_bar" name="Four Blocks Icon">
  <xpath expr="//div[@id='my_snippet']/div[@class='o_panel_body']" position="inside">
    <t t-snippet="my_snippet.s_four_blocks"
       t-thumbnail="/my_snippet/static/src/img/thumbnail.png"/>
  </xpath>
</template>

<template id="three_blocks_icon" inherit_id="my_snippet_bar" name="Three Blocks Icon">
  <xpath expr="//div[@id='my_snippet']/div[@class='o_panel_body']" position="inside">
    <t t-snippet="my_snippet.s_three_blocks"
       t-thumbnail="/my_snippet/static/src/img/thumbnail.png"/>
  </xpath>
</template>

<template id="image_and_four_blocks_icon" inherit_id="my_snippet_bar" name="Image And Four Blocks Icon">
  <xpath expr="//div[@id='my_snippet']/div[@class='o_panel_body']" position="inside">
    <t t-snippet="my_snippet.s_image_and_four_blocks"
       t-thumbnail="/my_snippet/static/src/img/thumbnail.png"/>
  </xpath>
</template>



<template id="four_blocks_and_image_icon" inherit_id="my_snippet_bar" name="Four Blocks And Image Icon">
  <xpath expr="//div[@id='my_snippet']/div[@class='o_panel_body']" position="inside">
    <t t-snippet="my_snippet.s_four_blocks_and_image"
       t-thumbnail="/my_snippet/static/src/img/thumbnail.png"/>
  </xpath>
</template>

</odoo>